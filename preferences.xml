<?xml version="1.0" encoding="UTF-8"?>
<preferences_structure xmlns="http://freeplane.sf.net/ui/preferences/1.0">
    <tabbed_pane>
        <tab name="plugins">
            <separator name="gpt_plugin">
                <string name="openai_key"/>
                <textbox name="gpt_prompt" lines="5"/>
                <number name="max_node_depth" min="0" max="10"  />
                <combo name="gpt_model">
                    <choice value="gpt-3.5-turbo"/>
                    <choice value="gpt-4"/>
                </combo>
            </separator>
        </tab>
    </tabbed_pane>
</preferences_structure>
